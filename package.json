{
  "name": "shelfcontrol",
  "main": "expo-router/entry",
  "version": "1.32.2",
  "scripts": {
    "start": "expo start -c --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "debug:android": "adb logcat | grep -E \"ReactNative|AndroidRuntime|DEBUG|ERROR|FATAL\"",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint src",
    "lint:fix": "expo lint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "typecheck": "npx tsc --noEmit",
    "prebuild": "npx expo prebuild --clean",
    "build:android": "node scripts/sync-version.js && npx eas build --platform android --profile preview",
    "build:ios": "node scripts/sync-version.js && npx eas build --platform ios",
    "submit:ios": "npx eas submit --platform ios --latest",
    "build:local": "npx eas build --platform android --profile preview --local",
    "sync-version": "node scripts/sync-version.js",
    "genTypes": "npx supabase gen types --lang=typescript --project-id \"npxxraogmtcaesjhzsun\" --schema public > src/types/database.types.ts",
    "test": "jest --runInBand",
    "test:ff": "jest --runInBand --bail",
    "test:coverage": "jest --runInBand --coverage",
    "test:coverage:watch": "jest --runInBand --coverage --watch",
    "test:debug": "jest --runInBand --detectOpenHandles --forceExit --no-cache --logHeapUsage",
    "test:e2e": "maestro test @.env.maestro maestro/e2e-main-flow.yaml"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/material-top-tabs": "^7.2.15",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.77.2",
    "@types/uuid": "^10.0.0",
    "aes-js": "^3.1.2",
    "base64-arraybuffer": "^1.0.2",
    "dayjs": "^1.11.13",
    "expo": "~53.0.23",
    "expo-apple-authentication": "~7.2.4",
    "expo-application": "~6.1.5",
    "expo-auth-session": "^6.1.5",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.6",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-secure-store": "~14.2.4",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "posthog-react-native": "^4.10.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.4",
    "react-native": "0.79.6",
    "react-native-calendars": "^1.1313.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-pager-view": "6.7.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "readable-stream": "^4.7.0",
    "stacktrace-parser": "^0.1.11",
    "uuid": "^13.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "latest",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.2.2",
    "@types/aes-js": "^3.1.4",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.ts"
    ],
    "globalTeardown": "<rootDir>/src/__tests__/teardown.ts",
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/setup.ts",
      "<rootDir>/src/__tests__/teardown.ts",
      "<rootDir>/src/__tests__/fixtures/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|isows|@supabase/.*)",
      "/node_modules/react-native-reanimated/plugin/"
    ],
    "clearMocks": true,
    "restoreMocks": true,
    "testTimeout": 10000,
    "detectOpenHandles": true,
    "workerIdleMemoryLimit": "512MB",
    "silent": false,
    "openHandlesTimeout": 1000,
    "collectCoverage": false,
    "verbose": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/__tests__/**/*",
      "!src/**/index.ts",
      "!src/app/**/*",
      "!src/types/**/*"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "private": true
}
